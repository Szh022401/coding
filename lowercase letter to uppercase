/*
lowercase letter to uppercase
*/
public static char toUp(char lower){
      return char(low - ' a' + 'A');
}

public static int  parseHex(char c){
      if(c >= '0' && c <= '9'){
            return c - '0';
      }
       if(c >= 'a' && c <= 'f'){
            return 10 + (c - 'a');
      }
      if(c >= 'A' && c <= 'F'){
            return 10 + (c - 'A');
      }
      return Integer.MAX_VAULE;
}

